{
  "name": "spotify-badge",
  "version": "1.1.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@7.3.0",
  "scripts": {
    "dev": "nodemon",
    "start": "vercel dev",
    "type-check": "tsc --noEmit",
    "format": "prettier --write '{**/*.{ts,json},.prettierrc}'"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0",
    "preact": "^10.8.1",
    "preact-render-to-string": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/spotify-api": "^0.0.16",
    "@vercel/node": "^2.2.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vercel": "^25.1.0"
  },
  "nodemonConfig": {
    "ext": "ts",
    "exec": "vercel dev",
    "watch": [
      "api/*",
      "components/*",
      "consts.ts",
      "lib/*"
    ]
  }
}
